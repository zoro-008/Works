                            <Button Name="btJogP" BorderThickness="0" Grid.Column="1" Grid.Row="0" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" PreviewMouseDown="BtJogP_PreviewMouseDown" PreviewMouseUp="BtJogP_PreviewMouseUp">
                                <StackPanel Name="pnJogP" Orientation="Horizontal">
                                    <TextBlock Name="tbJogP" Text="JOG +" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                    <materialDesign:PackIcon Name="piJogP" Kind="ArrowRight" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                </StackPanel>
                            </Button>
                            <Button Name="btStop" BorderThickness="0" Grid.Column="2" Grid.Row="0" Width="Auto" Height="Auto" Background="IndianRed" Margin="2" Click="BtStop_Click">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="StopCircleOutline" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="STOP" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btReset" BorderThickness="0" Grid.Column="0" Grid.Row="1" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" PreviewMouseDown="BtReset_PreviewMouseDown" PreviewMouseUp="BtReset_PreviewMouseUp">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Refresh" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="RESET" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btClearPos" BorderThickness="0" Grid.Column="1" Grid.Row="1" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtClearPos_Click">
                                <StackPanel Orientation="Horizontal" Margin="-8 0 0 0">
                                    <materialDesign:PackIcon Kind="Eraser" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="-5 0 5 0" />
                                    <TextBlock Text="CLEAR POS" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btHome" BorderThickness="0" Grid.Column="2" Grid.Row="1" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtHome_Click">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Home" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="HOME" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btServoOn" BorderThickness="0" Grid.Column="0" Grid.Row="2" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtServoOn_Click">
                                <StackPanel Orientation="Horizontal" Margin="-8 0 0 0">
                                    <materialDesign:PackIcon Kind="Motor" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="SERVO ON" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btServoOff" BorderThickness="0" Grid.Column="1" Grid.Row="2" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtServoOff_Click">
                                <StackPanel Orientation="Horizontal" Margin="-13 0 0 0">
                                    <materialDesign:PackIcon Kind="MotorOff" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="SERVO OFF" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btAllStop" BorderThickness="0" Grid.Column="2" Grid.Row="2" Width="Auto" Height="Auto" Background="IndianRed" Margin="2" Click="BtAllStop_Click">
                                <StackPanel Orientation="Horizontal" Margin="-13 0 0 0">
                                    <materialDesign:PackIcon Kind="StopCircleOutline" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="ALL STOP" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btServoOnAll" BorderThickness="0" Grid.Column="0" Grid.Row="3" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtServoOnAll_Click">
                                <StackPanel Orientation="Horizontal" Margin="-8 0 0 0">
                                    <materialDesign:PackIcon Kind="Motor" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="SERVO ON ALL" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btServoOffAll" BorderThickness="0" Grid.Column="1" Grid.Row="3" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtServoOffAll_Click">
                                <StackPanel Orientation="Horizontal" Margin="-10 0 0 0">
                                    <materialDesign:PackIcon Kind="MotorOff" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="SERVO OFF ALL" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                        </Grid>
                    </GroupBox>
<!--Motor Repeat=====================================================================================================================-->
                    <GroupBox Grid.Column="2" Grid.Row="3" Width="Auto" Height="Auto" Header="MOTOR REPEAT CONTROL" Margin="2 2 0 0">
                        <GroupBox.HeaderTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal" Height="15">
                                    <materialDesign:PackIcon Kind="Repeat" Height="20" Width="20" VerticalAlignment="Center" />
                                    <TextBlock Margin="8,0,0,0" VerticalAlignment="Center" Style="{StaticResource MaterialDesignSubheadingTextBlock}" Text="{Binding}" />
                                </StackPanel>
                            </DataTemplate>
                        </GroupBox.HeaderTemplate>

                        <Grid Margin="-6 -7 -6 -7">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="1*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                            </Grid.RowDefinitions>

                            <Button Name="btGo1stPos" BorderThickness="0" Grid.Column="0" Grid.Row="0" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtGo1stPos_Click">
                                <StackPanel Orientation="Horizontal" Margin="-9 0 0 0">
                                    <materialDesign:PackIcon Kind="ArrowExpandRight" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="GO 1st POS" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btGo2ndPos" BorderThickness="0" Grid.Column="1" Grid.Row="0" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtGo2ndPos_Click">
                                <StackPanel Orientation="Horizontal" Margin="-9 0 0 0">
                                    <materialDesign:PackIcon Kind="ArrowCompressRight" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="GO 2nd POS" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btRepeat" BorderThickness="0" Grid.Column="0" Grid.Row="1" Width="Auto" Height="Auto" Background="DarkCyan" Margin="2" Click="BtRepeat_Click">
                                <StackPanel Orientation="Horizontal" Margin="-9 0 0 0">
                                    <materialDesign:PackIcon Kind="Repeat" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="REPEAT" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>
                            <Button Name="btRpStop" BorderThickness="0" Grid.Column="1" Grid.Row="1" Width="Auto" Height="Auto" Background="IndianRed" Margin="2" Click="BtRpStop_Click">
                                <StackPanel Orientation="Horizontal" Margin="-9 0 0 0">
                                    <materialDesign:PackIcon Kind="StopCircleOutline" Height="25" Width="25" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 5 0" />
                                    <TextBlock Text="STOP" Margin="0,0,0,3" VerticalAlignment="Center" Foreground="White" FontSize="17"/>
                                </StackPanel>
                            </Button>


                        </Grid>
                    </GroupBox>
<!--Save Button=====================================================================================================================-->
                    <Button Name="btSaveMotr" BorderThickness="0" Grid.Column="3" Grid.Row="4" Width="142" Height="Auto" HorizontalAlignment="Right" VerticalAlignment="Stretch" Background="DarkCyan" Margin="0 2 0 0" Click="BtSaveMotr_Click">
                        <StackPanel>
                            <materialDesign:PackIcon Kind="ContentSave" Height="62" Width="62" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5 0 5 0" />
                            <TextBlock Margin="0,0,0,3" VerticalAlignment="Center" Text="SAVE" Foreground="White" FontSize="40"/>
                        </StackPanel>
                    </Button>

                </Grid>
            </TabItem>
<!--Motor=========================================================================================================================-->
        </TabControl>
    </Grid>
</Window>

                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="70"/>
                            </Grid.RowDefinitions>

                            <DockPanel Grid.Row="0" Background="LightSteelBlue">
                                <TextBlock Text=" NO" FontSize="18" VerticalAlignment="Center" Width="130"/>
                                <TextBox   x:Name="tbDoNo" FontSize="16" Background="Beige"  Text="1234" Padding="5 0 0 0" VerticalContentAlignment="Center"/>
                            </DockPanel>
                            <DockPanel Grid.Row="1" Background="LightSteelBlue" Margin="0 5 0 0" >
                                <TextBlock Text=" ENUM" FontSize="18" VerticalAlignment="Center" Width="130"/>
                                <TextBox   x:Name="tbDoEnum" FontSize="16" Background="Beige"  Text="1234" Padding="5 0 0 0" VerticalContentAlignment="Center"/>
                            </DockPanel>
                            <DockPanel Grid.Row="2" Background="LightSteelBlue" Margin="0 5 0 0" >
                                <TextBlock Text=" NAME" FontSize="18" VerticalAlignment="Center" Width="130"/>
                                <TextBox   x:Name="tbDoName" FontSize="16" Background="Beige"  Text="1234" Padding="5 0 0 0" VerticalContentAlignment="Center"/>
                            </DockPanel>
                            <DockPanel Grid.Row="3" Background="LightSteelBlue" Margin="0 5 0 0" >

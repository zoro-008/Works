<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
    </startup>
    <runtime>
      <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
        <probing privatePath="Dll;Dll\OmniDriverSPAM;DLL\Metro;Dll\SML"/>
      </assemblyBinding>
    </runtime>
    <appSettings>
      <add key="Serilog_Path" value="D:\Log\"/>
      <add key="Serilog_SeqUrl" value="http://localhost:5341"/>
      <add key="Serilog_Period" value="100"/>
      <add key="Serilog_Ignore" value="0"/>
    </appSettings>  
</configuration>
